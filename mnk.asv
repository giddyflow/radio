%% линейная регрессия мнк
%% входные параметры
x = 0:2:50;
y = x.^3;
k = 3; 
n = length(x);
%% расчёт 
X = zeros(n, k);
for i = 1:k
    X(:, i) = x.^(i-1); % Заполнение столбцов степенями x
end
b = X\y';
y_predict = X * b;

plot(y, 'LineWidth', 2);
hold on
plot(y_predict, 'LineWidth', 1)
grid on
hold off

